{
  "name": "vmrh",
  "description": "A Vue 3 package that integrates vue-router with modal state management, enabling modal dialogs to be controlled via routes.",
  "version": "0.1.0-alpha.1",
  "type": "module",
  "author": {
    "name": "shunnNet",
    "url": "https://github.com/shunnNet"
  },
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "play:vite": "pnpm --filter @vmrh/playground-vite dev",
    "play:build:vite": "pnpm --filter @vmrh/playground-vite build",
    "play:vite-uvr": "pnpm --filter @vmrh/playground-vite-uvr dev",
    "play:build:vite-uvr": "pnpm --filter @vmrh/playground-vite-uvr build",
    "build:core": "pnpm --filter @vmrh/core build",
    "stub:core": "pnpm --filter @vmrh/core stub",
    "dev:nuxt": "pnpm --filter @vmrh/nuxt dev",
    "prepare:nuxt": "pnpm --filter @vmrh/nuxt dev:prepare",
    "dev:build:nuxt": "pnpm --filter @vmrh/nuxt dev:build",
    "prepack:nuxt": "pnpm --filter @vmrh/nuxt prepack"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@stylistic/eslint-plugin": "^2.6.4",
    "@types/node": "^22.8.1",
    "@typescript-eslint/eslint-plugin": "8.15.0",
    "@typescript-eslint/parser": "8.15.0",
    "eslint-plugin-vue": "^9.27.0",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.9",
    "typescript": "^5.0.2",
    "typescript-eslint": "^8.2.0",
    "vue-tsc": "^1.8.15"
  },
  "files": [
    "dist"
  ],
  "pnpm": {
    "overrides": {
      "@typescript-eslint/eslint-plugin": "8.15.0",
      "@typescript-eslint/parser": "8.15.0"
    }
  }
}