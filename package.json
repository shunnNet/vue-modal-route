{
  "name": "vmr",
  "description": "VMR (Vue Modal Route) is a package that manages modals using routes.",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.umd.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "repository": "https://github.com/shunnNet/vrm",
  "bugs": {
    "url": "https://github.com/shunnNet/vrm/issues"
  },
  "keywords": [
    "vue",
    "modal",
    "vue-router"
  ],
  "author": "shunnNet",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "play:vite": "pnpm --filter @vmr/playground-vite dev",
    "play:build:vite": "pnpm --filter @vmr/playground-vite build",
    "play:vite-uvr": "pnpm --filter @vmr/playground-vite-uvr dev",
    "play:build:vite-uvr": "pnpm --filter @vmr/playground-vite-uvr build",
    "build:core": "pnpm --filter @vmr/vue-modal-route build",
    "stub:core": "pnpm --filter @vmr/vue-modal-route stub",
    "dev:nuxt": "pnpm --filter @vmr/nuxt dev",
    "prepare:nuxt": "pnpm --filter @vmr/nuxt dev:prepare",
    "dev:build:nuxt": "pnpm --filter @vmr/nuxt dev:build",
    "prepack:nuxt": "pnpm --filter @vmr/nuxt prepack"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@stylistic/eslint-plugin": "^2.6.4",
    "@types/node": "^22.8.1",
    "@typescript-eslint/eslint-plugin": "8.15.0",
    "@typescript-eslint/parser": "8.15.0",
    "eslint-plugin-vue": "^9.27.0",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.9",
    "typescript": "^5.0.2",
    "typescript-eslint": "^8.2.0",
    "vue-tsc": "^1.8.15"
  },
  "files": [
    "dist"
  ],
  "pnpm": {
    "overrides": {
      "@typescript-eslint/eslint-plugin": "8.15.0",
      "@typescript-eslint/parser": "8.15.0"
    }
  }
}
